extends layout
block links
    link(href="style/servicesDetail.css" rel="stylesheet")
block content
    doctype html
    html
        head
        body
            ul
                each item in casketsItem
                    each i in item
                        div(class='service_items')
                            li(id=i[0]) #{i[0]}
                            li(id=i[2]) #{i[1]}
                            li(id=i[3]) #{i[2]}
                            img.image(src=i[3], alt="", height="80px", width="160px")